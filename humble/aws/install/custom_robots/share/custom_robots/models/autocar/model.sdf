      <!--  <gravity>1</gravity> -->
      <!--  <gravity>1</gravity> -->
      <!-- <velocity_decay/> -->
      <self_collide>1</self_collide>
      <!--  <gravity>1</gravity> -->
    </link>

    <joint name='bl_axle' type='revolute'>
